{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
        "react", "import", "jsx-a11y"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
        "jsx": true
        }
    },
    "rules": {
        "react/prop-types": 0,
        "indent": ["error", 2],
        "linebreak-style": 0,
        "quotes": ["error", "single"],
        "ignorePureComponents": 0,
        "react/prefer-stateless-function": 0
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
